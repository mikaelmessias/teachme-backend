{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "env-cmd -f .env.nest nest start",
    "start:dev": "rm dist/ -rf && env-cmd -f .env.nest nest start --watch",
    "start:debug": "env-cmd -f .env.nest nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "env-cmd -f .env.typeorm node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:create": "env-cmd -f .env.typeorm node --require ts-node/register ./node_modules/typeorm/cli.js migration:create -d src/database/migration",
    "migration:run": "env-cmd -f .env.typeorm node --require ts-node/register ./node_modules/typeorm/cli.js migration:run"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.12",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.12",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/typeorm": "^8.0.3",
    "@types/graphql": "^14.5.0",
    "apollo-server-express": "^3.7.0",
    "dataloader": "^2.1.0",
    "env-cmd": "^10.1.0",
    "graphql": "^16.5.0",
    "graphql-tools": "^8.2.9",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sqlite3": "5.0.8",
    "type-graphql": "^1.1.1",
    "typeorm": "0.2.45",
    "voyager": "^0.4.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/node": "^16.11.10",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "10.4.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
